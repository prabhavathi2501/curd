/*CRUD => 
C-Create  (post)
R-Read  (get)
U-Update  (put) / (patch)
D-Delete  (delete)

const URL = "https://github.com/"
//post method
fetch(URL,{
    method: "post",
    body:{
        email, prabha@gmail.com
        password, 12345678
    }
})
//get method
fetch(URL)
   //put method
fetch(URL +"/"+id,{
method:"put",
body:{
    email,
    password,
}
})
//patch method
fetch(URL +"/"+id,{
method:"patch",
body:{
  password
    }
})
//delete method
